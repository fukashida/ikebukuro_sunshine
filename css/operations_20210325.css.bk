#shiwa_wrap .accident_flow li:nth-child(6):after {
	display: none;
}

#shiwa_wrap .accident_flow li .num {
	height: 130px;
	margin-bottom: 20px;
	position: relative;
	font-size: 16px;
	text-align: center;
	line-height: 1.2;
	box-sizing: border-box;
	padding: 5px 0 15px;
	display: block;
	font-weight: bold;
}

#shiwa_wrap .accident_flow li:nth-child(1) .num {
	background: #EBF4FF;
}

#shiwa_wrap .accident_flow li:nth-child(2) .num {
	background: #dfedff;
}

#shiwa_wrap .accident_flow li:nth-child(3) .num {
	background: #cee4ff;
}

#shiwa_wrap .accident_flow li:nth-child(4) .num {
	background: #C0DAF9;
}

#shiwa_wrap .accident_flow li:nth-child(5) .num {
	background: #abd2ff;
}

#shiwa_wrap .accident_flow li:nth-child(6) .num {
	background: #94c5ff;
}

#shiwa_wrap .accident_flow li .num img {
	margin-bottom: 15px;
	max-width: 32px;
	height: auto;
}

#shiwa_wrap .accident_flow li .num:after {
	content: "";
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 65px 0 65px 15px;
	position: absolute;
	right: -15px;
	top: 0;
	z-index: 10;
}

#shiwa_wrap .accident_flow li:nth-child(1) .num:after {
	border-color: transparent transparent transparent #EBF4FF;
}

#shiwa_wrap .accident_flow li:nth-child(2) .num:after {
	border-color: transparent transparent transparent #dfedff;
}

#shiwa_wrap .accident_flow li:nth-child(3) .num:after {
	border-color: transparent transparent transparent #cee4ff;
}

#shiwa_wrap .accident_flow li:nth-child(4) .num:after {
	border-color: transparent transparent transparent #C0DAF9;
}

#shiwa_wrap .accident_flow li:nth-child(5) .num:after {
	border-color: transparent transparent transparent #abd2ff;
}

#shiwa_wrap .accident_flow li:nth-child(6) .num:after {
	border-color: transparent transparent transparent #94c5ff;
}

#shiwa_wrap .accident_flow li:after {
	content: "\025bc";
	bottom: 16px;
	color: #ffffff;
	left: 49%;
	position: absolute;
}

.arrow_down {
	position: relative;
	padding-left: 50px;
}

/* 長方形 */
.arrow_down::before {
	content: "";
	position: absolute;
	left: 15px;
	top: 0;
	width: 21px;
	height: 95%;
	background-color: #E9E7E8;
}

/* 三角 */
.arrow_down::after {
	content: "";
	position: absolute;
	top: 95%;
	left: 5px;
	width: 0;
	height: 0;
	border-top: 20px solid #E9E7E8;
	border-left: 20px solid transparent;
	border-right: 20px solid transparent;
}

/*----- 交通事故 保険 -----*/
#shiwa_wrap .hoken {
	margin-top: 20px;
	padding: 15px;
	background-color: #a29584;
}

#shiwa_wrap .hoken h3 {
	margin-bottom: 20px;
	text-align: center;
	font-weight: bold;
	font-size: 1.4em;
	color: #ffffff;
}

#shiwa_wrap .hoken ul {}

#shiwa_wrap .hoken ul li {
	width: 49%;
	border-radius: 10px;
	padding: 10px 10px 0 10px;
	background-color: #fff;
	box-sizing: border-box;
}

#shiwa_wrap .hoken ul li h4 {
	margin: 10px 0;
	color: #a29584;
	text-align: center;
	font-weight: bold;
	font-size: 1.2em;
}

#shiwa_wrap .hoken ul li.left {
	float: left;
	margin-right: 2%;
}

#shiwa_wrap .hoken ul li.right {
