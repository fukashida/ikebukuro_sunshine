	background: url(https://www.ike-sunshine.co.jp/wp-content/uploads/2021/03/bg-visit.jpg) no-repeat center;
	background-size: cover;
	padding: 20px;
}

/* WebP対応ブラウザ */
.webp .laser_operation #flow {
	background: url(https://www.ike-sunshine.co.jp/wp-content/uploads/2021/03/bg-visit.jpg.webp) no-repeat center;
	background-size: cover;
	padding: 20px;
}

.laser_operation #flow .box {
	/*	height: 37rem;
	background-size: 100%;*/
	display: flex;
	justify-content: space-between;
	position: relative;
	background: #ffffff;
	margin-bottom: 50px;
	padding: 30px;
	box-shadow: 1px 1px 10px rgba(0, 0, 0, 0.1);
}

.laser_operation #flow .box .txt {
	width: 65%;
	/*	position: relative;
	top: 2rem;*/
	font-size: 1.8rem;
}

.laser_operation #flow .box .number {
	position: absolute;
	font-family: "游明朝", YuMincho, "ヒラギノ明朝 ProN W3", "Hiragino Mincho ProN", "HG明朝E", "ＭＳ Ｐ明朝", "ＭＳ 明朝", serif;
	font-style: italic;
	font-size: 9em;
	font-weight: bold;
	line-height: 1;
	top: 5px;
	left: 5px;
	color: rgba(0, 0, 0, 0.1);
}

.laser_operation #flow .box .txt .ttl {
	font-size: 3rem;
	line-height: 1.2;
	color: #563731;
	margin-left: 3.2em;
	/*	padding: 1rem 0;*/
	margin-bottom: 2rem;
}

.laser_operation #flow .box .txt p {
	margin-bottom: 0;
}

.laser_operation #flow .box .img {
	width: 32%;
	/*	margin: 4% 0;*/
}

.laser_operation #flow .box .img img {
	width: 100%;
	height: 25rem;
	object-fit: cover;
}

.laser_operation #flow .box:after {
	border-top: 40px solid #ffffff;
	border-left: 40px solid transparent;
	border-right: 40px solid transparent;
	content: "";
	position: absolute;
	bottom: -40px;
	/* 三角形の高さ＋ボックスのボーダーをマイナスに */
	left: 50%;
	/* 中央寄せに使用 */
	margin-left: -50px;
	/* 中央寄せに使用 */
}

.laser_operation #flow .box:last-child {
	margin-bottom: 0;
}

.laser_operation #flow .box:last-child:after {
	border: none;
}

