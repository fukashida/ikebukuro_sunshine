}

/*----- 交通事故 よくある質問 -----*/
#shiwa_wrap #question {
	margin-bottom: 40px;
}

#shiwa_wrap #question h2 {
	margin-bottom: 20px;
	padding-bottom: 20px;
	border-bottom: 1px dotted #cea253;
	text-align: center;
}

#shiwa_wrap #question dl {
	padding: 0;
}

#shiwa_wrap #question dl dt {
	min-height: 25px;
	padding-left: 42px;
	background: url(../img/operations/shiwa/question_q.png) no-repeat left;
}

#shiwa_wrap #question dl dt span {
	display: block;
	padding: 20px;
	border-left: 1px dotted #e69585;
	color: #e69585;
	font-weight: bold;
	font-size: 1.1em;
}

#shiwa_wrap #question dl dd {
	min-height: 25px;
	padding-left: 42px;
	background: url(../img/operations/shiwa/question_a.png) no-repeat left;
}

#shiwa_wrap #question dl dd span {
	display: block;
	padding: 20px;
	border-left: 1px dotted #ccc;
}

