	text-align: center;
	line-height: 1.4;
	box-sizing: border-box;
	padding: 10px 0;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(1) .num {
	background: #EBF4FF;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(2) .num {
	background: #C0DAF9;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(3) .num {
	background: #9AC9F7;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(4) .num {
	background: #7DB0EF;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(5) .num {
	background: #659AE5;
}

.wakiga_operations .wakigaTreatment ul li .num:after {
	content: "";
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 50px 0 50px 15px;
	position: absolute;
	right: -15px;
	top: 0;
	z-index: 10;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(1) .num:after {
	border-color: transparent transparent transparent #EBF4FF;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(2) .num:after {
	border-color: transparent transparent transparent #C0DAF9;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(3) .num:after {
	border-color: transparent transparent transparent #9AC9F7;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(4) .num:after {
	border-color: transparent transparent transparent #7DB0EF;
}

.wakiga_operations .wakigaTreatment ul li:nth-child(5) .num:after {
	border-color: transparent transparent transparent #659AE5;
}

