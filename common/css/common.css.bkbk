}
.menuList.long li {
	width: 100%;
	float: none;
}
.menuList.half li {
	width: 50%;
}
.menuList li a {
	border: 2px solid #F4F2F3;
	padding: 10px 20px 10px 10px;
	display: block;
	position: relative;
	color: #822520;
}
.menuList li a::after {
	content: "\e804";
	position: absolute;
	font-family: "fontello";
	top: 50%;
	margin-top: -11px;
}
.menuList li a:hover {
	background-color: #F4F2F3;
}
.menuList li.beside a::after {
	content: "\e803";
	position: absolute;
	font-family: "fontello";
	line-height: inherit;
}

/*ページ上部コンテンツナビ*/
.menuList.head li a {
	border: none;
	padding: 0;
	display: table;
	width: 100%;
}
.menuList li a::after {
	right: 10px;
}
