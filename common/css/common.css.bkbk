@charset "utf-8";

/* CSS Document */
@import url(//fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,700,700italic);
@import url(//fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700);

/*---------------------------------------------------------------
  link
----------------------------------------------------------------*/
a:link {
	text-decoration: none;
	color: #82231F;
}
a:visited {
	text-decoration: none;
	color: #82231F;
}
a:hover {
	text-decoration: none;
	color: #e0807c;
}
a:hover img {
	filter: alpha(opacity=70);
	-moz-opacity: 0.7;
	opacity: 0.7;
}

/*---------------------------------------------------------------
  base
----------------------------------------------------------------*/
html {
	font-size: 62.5%; /*10px*/
}
body {
	color: #333;
	font-family: Avenir, "Open Sans", "Helvetica Neue", Helvetica, Arial, Verdana, Roboto, "游ゴシック", "Yu Gothic", "游ゴシック体", "YuGothic", "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", "メイリオ", Meiryo, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif;
	font-size: 14px;
	font-size: 1.4rem;
	line-height: 1.6;
	-webkit-text-size-adjust: 100%;
	-webkit-font-smoothing: antialiased;
	background: url(/common/img/bg_body.png) left top repeat-x;
}
table {
	font-size: 100%;
}

/*---------------------------------------------------------------
  spacing
----------------------------------------------------------------*/
.mb05 {
	margin-bottom: 5px !important;
}
.mb10 {
	margin-bottom: 10px !important;
}
.mb15 {
	margin-bottom: 15px !important;
}
.mb20 {
	margin-bottom: 20px !important;
}
.mb25 {
	margin-bottom: 25px !important;
}
.mb30 {
	margin-bottom: 30px !important;
}
.mb35 {
	margin-bottom: 35px !important;
}
.mb40 {
	margin-bottom: 40px !important;
}
.mb45 {
	margin-bottom: 45px !important;
}
.mb50 {
	margin-bottom: 50px !important;
}
.mb60 {
	margin-bottom: 60px !important;
}
.mr15 {
	margin-right: 15px !important;
}
.notes {
	font-weight: bold;
	font-size: 14px;
	font-size: 1.4rem;
}

/*---------------------------------------------------------------
  clear
----------------------------------------------------------------*/
.clr {
	clear: both;
}
.clrfix:after {
	content: ".";
	display: block;
	height: 0;
	font-size: 0;
	clear: both;
	visibility: hidden;
}
.clrfix {
	display: inline-block;
}

/* Hides from IE Mac */
* html .clrfix {
	height: 1%;
}
.clrfix {
	display: block;
}

/* End Hack */
.pc_hide {
	display: none;
}
.disnon {
	display: none;
}
#header {
	width: 100%;
	background: url(/common/img/wood.png);
}
header {
	width: 980px;
	position: relative;
	margin: 0 auto;
	height: 131px;
}
#headText {
	font-size: 1px;
	font-size: 1rem;
	line-height: 1;
	position: absolute;
	top: 12px;
	padding-right: 147px;
	color: #fff;
}
#headNav {
	position: absolute;
	line-height: 1;
	top: 10px;
	right: 0;
	border-left: 1px solid #333;
	display: none;
}
#headNav li {
	float: left;
	border-right: 1px solid #333;
	padding: 0 29px;
	font-size: 12px;
	font-size: 1.2rem;
}
#headNav li a {
	color: #333;
}
#gnav {
	position: absolute;
	left: 0;
	width: 100%;
	bottom: 10px
}
#gnav ul {
	margin: 0 -2px;
}
#gnav li {
	width: 14.28%;
	float: left;
	box-sizing: border-box;
	padding: 0 1px;
	height:30px;
}
#gnav li a {
	display: block;
	height:30px;
    line-height: 30px;
    color: #82231F;
    text-align: center;
    background: -moz-linear-gradient( top, #ffffff 0%, #ffffff 50%, #d6d6d6);
    background: -webkit-gradient( linear, left top, left bottom, from(#ffffff), color-stop(0.50, #ffffff), to(#d6d6d6));
	background:linear-gradient(to bottom, #ffffff, #ffffff, #d6d6d6); /* IE */
    -moz-box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 3px rgba(255,255,255,1);
    -webkit-box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 3px rgba(255,255,255,1);
    box-shadow: 0px 1px 3px rgba(000,000,000,0.5), inset 0px 0px 3px rgba(255,255,255,1);
    text-shadow: 0px -1px 0px rgba(000,000,000,0.1), 0px 1px 0px rgba(255,255,255,1);
	/* IE8 IE9 */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(GradientType=0,startcolorstr=#ffffff, endcolorstr=#d6d6d6))";
	zoom: 1;
}

#gnav li a:hover {
	opacity: .8;
}
#headLogo {
	width: 471px;
	height: 49px;
	position: absolute;
	left: 4px;
	top: 39px;
	background: url(../img/logo01.png) no-repeat;
	overflow: hidden;
	text-indent: -9999px;
}
#headLogo a {
	display: block;
}
#headTel {
	position: absolute;
	top: 33px;
	right: 155px;
}
#headContact {
	position: absolute;
	top: 33px;
	right: 0;
}
#headContact li {
	margin-bottom: 4px;
	position: relative;
}
#headContact li img {
	width: auto;
	height: auto;
}
#headContact li a {
	background-color: #FBA733;
	display: block;
	color: #FFF;
	text-align: center;
	padding: 0 13px;
}
#headContact li a:after {
	content: "";
	display: block;
	width: 5px;
	height: 5px;
	background: url(../img/ic_btarrow.png);
	position: absolute;
	bottom: 4px;
	right: 2px;
}
#headContact li a:hover {
	filter: alpha(opacity=70);
	-moz-opacity: 0.7;
	opacity: 0.7;
}
#footer {
	clear: both;
	width: 100%;
	background: #F4F2F3 url(../img/bg_footer.gif) repeat-x left top;
	position: relative;
}
#footer .inner {
	clear: both;
	width: 980px;
	margin: 0 auto;
	position: relative;
}
#footHead {
	padding: 15px 0
}
#footHead .inner {
	clear: both;
	width: 960px;
	margin: 0 auto;
}
#footHead .name {
	font-size: 18px;
	font-size: 1.8rem;
	color: #7F251D;
	line-height: 20px;
	float: left;
	margin: 0 20px 0 0;
}
#footHead .tel, #footHead .tel dt, #footHead .tel dd {
	float: left;
	line-height: 20px;
}
#footHead .tel dt {
	font-size: 17px;
	font-size: 1.7rem;
	font-weight: bold;
	color: #5D5B5C;
	font-family: "ヒラギノ明朝 Pro W3", "Hiragino Mincho Pro", "ＭＳ Ｐ明朝", "MS PMincho", serif;
	margin: 0 10px 0 0;
	background: url(../img/ic_freedial.gif) no-repeat left center;
	padding: 0 0 0 36px;
}
#footNav {
	padding: 35px 20px 35px 0;
}
#footNav .box {
	float: left;
	margin-left: 100px;
}
#footNav .box:first-child {
	margin: 0;
}
#footNav .box ul {
	margin: 0 0 1.5em;
}
#footNav .box li {
	line-height: 1;
	margin: 0 0 10px;
}
#footNav .box li a {
	font-size: 13px;
	font-size: 1.3rem;
}
#footNav .box li a::before {
	content: "\e800";
	font-family: "fontello";
	padding-right: 5px;
}
#gotoTop {
	position: absolute;
	right: 0;
	top: 15px;
	line-height: 20px;
}
#gotoTop a:after {
	content: "\e801";
	font-family: "fontello";
	padding-left: 5px;
}
#copyright {
	position: absolute;
	right: 20px;
	bottom: 20px
}

/*column layout*/
.clmBox {
	clear: both;
	margin: 0 -12px;
	position: relative;
}
.clmBox::after {
	content: "";
	clear: both;
	display: block;
}
.clmBox > .box {
	float: left;
	width: 50%;
	box-sizing: border-box;
	padding: 0 12px;
}
.clmBox.box2 > .box {
	width: 50%;
}
.clmBox.box3 > .box {
	width: 33.33333%;
}
.clmBox.box4 > .box {
	width: 25%;
}
.clmBox.box5 > .box {
	width: 20%;
}
#sideReservationWrap {
	position: absolute;
	top: 0;
	left: 50%;
	width: 920px;
	margin: 0 0 0 -440px;
}
@media screen and (max-width:1400px) {
	#sideReservationWrap {
		display: none;
	}
}
#sideReservationWrap.fixed {
	position: fixed;
}
#sideReservation {
	position: absolute;
	right: -244px;
	top: 15px;
	border: 2px solid #832420;
	background: #F4F2F3;
	width: 220px;
	padding: 15px 15px 10px;
	box-sizing: border-box;
}
#sideReservationWrap.fixed #sideReservation {
	top: 15px;
}
#sideReservation .tel {
	text-align: center;
}
#sideReservation .tel span {
	display: inline-block;
	font-size: 22px;
	font-size: 2.2rem;
	font-family: "ヒラギノ明朝 Pro W3", "Hiragino Mincho Pro", "ＭＳ Ｐ明朝", "MS PMincho", serif;
	font-weight: bold;
	color: #5C5B5B;
	background: url(../img/ic_freedial.gif) no-repeat left center;
	padding: 0 0 0 35px;
}
#sideReservation .time {
	text-align: center;
	line-height: 1.3;
	font-size: 14px;
	font-size: 1.4rem;
	margin: 0 0 1em
}
#sideReservation .linkBtn01 {
	margin-bottom: 25px;
}
#sideReservation .linkBtn01 li {
	margin: 0 0 10px
}
#sideReservation .time span {
	display: block;
}
#sideReservation .search {
	text-align: center;
}
#sideReservation .search dt {
	margin: 0 0 5px;
	font-size: 14px;
	font-size: 1.4rem;
	line-height: 1;
}
#sideReservation .search dd input[type="text"] {
	width: 100%;
	box-sizing: border-box;
	border: 1px solid #848484;
	font-size: 14px;
	font-size: 1.4rem;
	padding: 5px;
}
#sideReservation .search dd input[type="submit"] {
	background: #82231F;
	color: #FFF;
	border: none;
	text-align: center;
	padding: 5px 10px;
	margin: 10px 0 0;
}
#container {
	clear: both;
	width: 980px;
	margin: 0 auto;
	position: relative;
	background: #fff;
	padding: 10px 0 20px;
}
.breadcrumbs {
	clear: both;
	line-height: 30px;
	padding: 0 15px;
	margin: 0 0 10px;
}
.breadcrumbs li {
	float: left;
}
.breadcrumbs li::before {
	content: "\e803";
	margin: 0 .5em;
	font-family: "fontello";
}
.breadcrumbs li:first-child::before {
	display: none;
}
#mainContents {
	width: 690px;
	float: right;
	margin-bottom: 50px;
	margin-right: 15px;
}
#subContents {
	width: 240px;
	float: left;
	background: #F4F2F3;
	min-height: 500px;
	box-sizing: border-box;
	padding: 20px 20px 0;
	margin-bottom: 50px;
	margin-left: 15px;
}
#subContents h3 {
	position: relative;
	width: 100%;
	text-align: center;
	line-height: 1;
	margin: 0 0 15px;
}
#subContents h3 span {
	display: inline-block;
	padding: 0 9px;
	position: relative;
	z-index: 1;
	background: #F4F2F3;
	min-width: 4em;
	color: #82231F;
	font-size: 16px;
	font-size: 1.6rem;
	font-weight: bold;
}
#subContents h3::before, #subContents h3::after {
	content: "";
	position: absolute;
	width: 100%;
	height: 1px;
	top: 50%;
	left: 0;
	background: #82231F;
}
#subContents h3::before {
	margin-top: -3px;
}
#subContents h3::after {
	margin-bottom: -3px;
}
#subContents .list01 {
	margin-bottom: 30px;
}
#subContents .list01 li {
	margin: 0 0 .5em;
}
#subContents .list01 a {
	display: block;
	line-height: 1.2;
	position: relative;
	padding-left: 10px;
}
#subContents .list01 a::before {
	content: "\e800";
	font-family: "fontello";
	position: absolute;
	left: 0;
	top: 0;
	bottom: 0;
	margin: auto 0;
}
#subContents .bnr {
	margin-bottom: 30px;
}
#subContents .bnr li {
	margin: 0 0 8px;
}
#subContents .bnr + .bnr {
	margin-top: -10px;
	padding-top: 13px;
	border-top: 1px solid #DDD7D9;
}
#sideGototop {
	text-align: center;
	line-height: 1;
	margin-top: 30px;
}
#sideGototop a::after {
	content: "\e801";
	font-family: "fontello";
	padding-left: 5px;
}

